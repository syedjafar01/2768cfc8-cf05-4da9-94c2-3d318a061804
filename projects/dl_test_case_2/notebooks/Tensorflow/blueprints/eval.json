{
  "version": "3.0.0",
  "description": "To get the metrics on the data input",
  "name": "DLEvalBlock",
  "component_id": "evalFlowComponentId",
  "inputs": [
    {
      "render_type": "Text",
      "name": "batch_size",
      "required": true,
      "description": "Batch Size",
      "default": 10,
      "hidden": false,
      "label": "Batch Size",
      "supportVar": false,
      "dataType": "integer",
      "dataFlowType": "atomic"
    },
    {
      "render_type": "SELECT_MODEL_FROM_MLC",
      "name": "model",
      "required": true,
      "description": "Eval flow Default Input",
      "hidden": false,
      "label": "Trained Model",
      "supportVar": false,
      "dataType": "json",
      "dataFlowType": "atomic"
    },
    {
      "render_type": "MLC_SELECT_METRICS",
      "name": "metrics",
      "required": true,
      "description": "metrics",
      "request_definition": {
        "call_url": "/rztai/mlc/:projectId/model_run/Platform/model/block_detail/METRIC,COST",
        "call_body": {
          "key": [
            "model",
            "value",
            "key"
          ]
        }
      },
      "hidden": false,
      "label": "Metrics",
      "supportVar": false,
      "dataType": "json",
      "dataFlowType": "atomic"
    },
    {
      "render_type": "NO_PROPERTY_DENT",
      "name": "data",
      "required": true,
      "hidden": false,
      "label": "Data",
      "supportVar": false,
      "dataType": "generic",
      "dataFlowType": "series"
    },
    {
      "render_type": "Text",
      "name": "load_path",
      "required": false,
      "description": "Load Path",
      "default": "default_load_path",
      "hidden": true,
      "label": "Load Path",
      "supportVar": false,
      "dataType": "atomic",
      "dataFlowType": "atomic"
    }
  ],
  "outputs": [
    {
      "render_type": "NO_PROPERTY_DENT",
      "name": "eval_output",
      "required": true,
      "description": "Eval Output",
      "hidden": false,
      "label": "Eval Output",
      "supportVar": false,
      "dataType": "generic",
      "dataFlowType": "atomic"
    }
  ],
  "technology": "PYTHON",
  "label": "Dl Eval Block",
  "resourceSpec": {
    "cores": 2,
    "gpu": 0,
    "kafkaRequired": false,
    "memory": 2048,
    "runEnv": "CPU",
    "useGpu": false,
    "useGpuIfAvailable": false
  }
}